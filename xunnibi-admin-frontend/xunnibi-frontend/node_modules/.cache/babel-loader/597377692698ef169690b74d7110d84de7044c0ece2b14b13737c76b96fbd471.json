{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ElMessage, ElMessageBox } from 'element-ui';\nexport default {\n  data() {\n    return {\n      count: 5,\n      isRegister: false,\n      registerForm: {\n        uname: '',\n        pwd: null,\n        secondpwd: '',\n        telephone: ''\n      }\n    };\n  },\n  created() {\n    this.count = 5;\n  },\n  methods: {\n    toRegister() {\n      ElMessageBox.alert('请牢记您的账号和密码！<br> 您的账号是：2041036529<br>您的密码是：792364@qq.com', '提示', {\n        dangerouslyUseHTMLString: true,\n        draggable: true,\n        overflow: true,\n        showClose: false,\n        confirmButtonText: '确定',\n        type: 'warning'\n      }).then(() => {\n        this.startCountdown();\n        this.isRegister = true;\n      });\n    },\n    goBack() {\n      if (this.count !== -1) {\n        this.count = -1;\n      }\n      this.$router.push({\n        path: '/login'\n      });\n      this.isRegister = false;\n    },\n    startCountdown() {\n      const interval = setInterval(() => {\n        if (this.count > 0) {\n          this.count--;\n        } else {\n          clearInterval(interval);\n          this.goBack();\n        }\n      }, 1000);\n    },\n    toReset() {\n      this.registerForm = {\n        uname: '',\n        pwd: 1,\n        secondpwd: '',\n        telephone: ''\n      };\n    }\n  }\n};","map":{"version":3,"names":["ElMessage","ElMessageBox","data","count","isRegister","registerForm","uname","pwd","secondpwd","telephone","created","methods","toRegister","alert","dangerouslyUseHTMLString","draggable","overflow","showClose","confirmButtonText","type","then","startCountdown","goBack","$router","push","path","interval","setInterval","clearInterval","toReset"],"sources":["src/views/RegisterView.vue"],"sourcesContent":["<template>\n    <div class=\"register-body\">\n        <div class=\"register-content\">\n            <el-card v-if=\"!isRegister\" class=\"register-card\" :body-style=\"{ padding: '0 20px' }\">\n                <template #header>\n                    <h2 class=\"header-title\">管理员注册</h2>\n                </template>\n                <div class=\"register-form\">\n                    <div class=\"back\" @click=\"goBack\">\n                        <i class=\"el-icon-caret-left\">返回</i>\n\n                    </div>\n\n                    <el-form v-model=\"registerForm\" label-width=\"100px\">\n                        <el-form-item prop=\"uname\" class=\"custom-form-item\" label=\"用户名:\">\n                            <el-input type=\"text\" v-model=\"registerForm.uname\" placeholder=\"请输入姓名\" />\n                        </el-form-item>\n                        <el-form-item label=\"密码：\" prop=\"pwd\">\n                            <el-input type=\"text\" v-model=\"registerForm.email\" placeholder=\"请输入邮箱\" />\n                        </el-form-item>\n                        <el-form-item label=\"确认密码:\" prop=\"secondpwd\">\n                            <el-input type=\"text\" v-model=\"registerForm.email\" placeholder=\"请输入邮箱\" />\n                        </el-form-item>\n                        <el-form-item label=\"常用手机：\" prop=\"telephone\">\n                            <el-input type=\"text\" v-model=\"registerForm.telephone\" placeholder=\"请输入手机号\" />\n                        </el-form-item>\n                        <div class=\"form-buttons\">\n                            <el-button color=\"#4AD9FD\" type=\"primary\" @click=\"toRegister\">注册</el-button>\n                            <el-button color=\"#5941a6\" type=\"warning\" @click=\"toReset\">重置</el-button>\n                        </div>\n                    </el-form>\n                </div>\n            </el-card>\n\n            <!-- Success Message -->\n            <el-card v-else class=\"register-card\" :body-style=\"{ padding: '0 20px' }\">\n                <el-result icon=\"success\" title=\"注册成功\" :sub-title=\"`${count}秒后自动返回登陆页面`\">\n                    <template #extra>\n                        <el-button type=\"primary\" @click=\"goBack\">返回</el-button>\n                    </template>\n                </el-result>\n            </el-card>\n        </div>\n    </div>\n</template>\n\n<script>\nimport { ElMessage, ElMessageBox } from 'element-ui';\n\nexport default {\n    data() {\n        return {\n            count: 5,\n            isRegister: false,\n            registerForm: {\n                uname: '',\n                pwd: null,\n                secondpwd: '',\n                telephone: '',\n\n            }\n        };\n    },\n    created() {\n        this.count = 5;\n    },\n    methods: {\n        toRegister() {\n            ElMessageBox.alert(\n                '请牢记您的账号和密码！<br> 您的账号是：2041036529<br>您的密码是：792364@qq.com',\n                '提示',\n                {\n                    dangerouslyUseHTMLString: true,\n                    draggable: true,\n                    overflow: true,\n                    showClose: false,\n                    confirmButtonText: '确定',\n                    type: 'warning'\n                }\n            ).then(() => {\n                this.startCountdown();\n                this.isRegister = true;\n            });\n        },\n        goBack() {\n            if (this.count !== -1) {\n                this.count = -1;\n            }\n            this.$router.push({ path: '/login' });\n            this.isRegister = false;\n        },\n        startCountdown() {\n            const interval = setInterval(() => {\n                if (this.count > 0) {\n                    this.count--;\n                } else {\n                    clearInterval(interval);\n                    this.goBack();\n                }\n            }, 1000);\n        },\n        toReset() {\n            this.registerForm = {\n                uname: '',\n                pwd: 1,\n                secondpwd: '',\n                telephone: '',\n            };\n        }\n    }\n};\n</script>\n\n<style lang=\"less\" scoped>\n/* General Styles */\n.register-body {\n    height: 100vh;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    background-color: #f4f7fc;\n    font-family: 'Arial', sans-serif;\n    /* 更优雅的字体 */\n}\n\n.register-content {\n    width: 100%;\n    max-width: 500px;\n    padding: 20px;\n}\n\n.register-card {\n    border-radius: 12px;\n    box-shadow: 0 8px 20px rgba(0, 0, 0, 0.1);\n    margin-top: 20px;\n}\n\n.header-title {\n    font-size: 26px;\n    font-weight: bold;\n    color: #333;\n    text-align: center;\n    padding: 15px 0;\n\n}\n\n.register-form {\n    padding: 20px;\n}\n\n.el-form-item {\n    margin-bottom: 20px;\n}\n\n.el-input {\n    font-size: 16px;\n    padding: 12px;\n    border-radius: 8px;\n}\n\n.el-radio-group {\n    display: flex;\n    justify-content: space-between;\n}\n\n.el-button {\n    font-size: 16px;\n    padding: 12px;\n    border-radius: 8px;\n    width: 48%;\n}\n\n.form-buttons {\n    display: flex;\n    justify-content: space-between;\n    margin-top: 25px;\n}\n\n.back {\n    cursor: pointer;\n    display: flex;\n    align-items: center;\n    color: #4a90e2;\n    font-size: 16px;\n    margin-bottom: 15px;\n}\n\n.back i {\n    margin-right: 5px;\n}\n\n.el-result .el-button {\n    width: 100%;\n    font-size: 16px;\n    padding: 12px;\n    border-radius: 8px;\n}\n\n.el-result {\n    text-align: center;\n    padding: 20px;\n}\n\n.el-result .el-button {\n    font-size: 16px;\n    padding: 12px;\n    border-radius: 8px;\n}\n</style>\n"],"mappings":";AA+CA,SAAAA,SAAA,EAAAC,YAAA;AAEA;EACAC,KAAA;IACA;MACAC,KAAA;MACAC,UAAA;MACAC,YAAA;QACAC,KAAA;QACAC,GAAA;QACAC,SAAA;QACAC,SAAA;MAEA;IACA;EACA;EACAC,QAAA;IACA,KAAAP,KAAA;EACA;EACAQ,OAAA;IACAC,WAAA;MACAX,YAAA,CAAAY,KAAA,CACA,2DACA,MACA;QACAC,wBAAA;QACAC,SAAA;QACAC,QAAA;QACAC,SAAA;QACAC,iBAAA;QACAC,IAAA;MACA,CACA,EAAAC,IAAA;QACA,KAAAC,cAAA;QACA,KAAAjB,UAAA;MACA;IACA;IACAkB,OAAA;MACA,SAAAnB,KAAA;QACA,KAAAA,KAAA;MACA;MACA,KAAAoB,OAAA,CAAAC,IAAA;QAAAC,IAAA;MAAA;MACA,KAAArB,UAAA;IACA;IACAiB,eAA